<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.lanchui.commonBiz.bean.lg.ShopInfo" table="oms_shop_info" lazy="false">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="shopNumber" column="shop_number" type="string"/>
        <property name="createDate" column="create_date" type="java.util.Date"/>
        <property name="marketId"  column="market_id" type="int"/>
        <property name="marketIdUuid"  column="market_id_uuid" type="int"/>
        <property name="marketName"  column="market_name" type="string"/>
        <property name="companyId"  column="company_id" type="int"/>
        <property name="dealerId" column="dealer_id"  type="int"/>
        <property name="brandId"  column="brand_id" type="int"/>
        <property name="brandName" column="brand_name"  type="string"/>
        <property name="seriesId"  column="series_id"  type="int"/>
        <property name="seriesName"  column="series_name"  type="string"/>
        <property name="isDel"  column="is_del"  type="int"/>
        <property name="status"  column="status" type="int"/>
        <property name="marketBoothNumber"  column="market_booth_number"  type="string"/>
        <property name="marketFloor"  column="market_floor"  type="string"/>
        <property name="shopTel"  column="shop_tel"  type="string"/>
        <property name="salesStartTime"  column="sales_start_time"  type="java.util.Date"/>
        <property name="salesEndTime" column="sales_end_time"  type="java.util.Date"/>
        <property name="shopName" column="shop_name"  type="java.lang.String"/>
        <property name="shopIntroduction"  column="shop_introduction" type="string"/>
        <property name="supplierInfo" column="supplier_info"  type="string"/>
    </class>

</hibernate-mapping>
