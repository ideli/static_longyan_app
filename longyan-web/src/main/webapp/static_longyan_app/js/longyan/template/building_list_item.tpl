<div class="building-list-item" data-value="<#=data.id#>">
	<div class="item-col-1">
		<div class="item-building-no" data-value="<#=data.building_no#>"><#=data.building_no#> 号楼</div>	
		<div class="item-building-detail">
			<div class="item-floor-amount" data-value="<#=data.floor_amount#>"><#=data.floor_amount#> 层楼</div>	
			<div class="item-unit-amount" data-value="<#=data.unit_amount#>"><#=data.unit_amount#> 个单元</div>	
			<div class="item-room-amount" data-value="<#=data.room_amount#>"><#=data.room_amount#> 户</div>	
			<div class="clear-both item-community-id" data-value="<#=data.community_id#>"></div>
		</div>			
	</div>
	<div class="item-col-2"></div>
	<div class="clear-both"></div>
</div>