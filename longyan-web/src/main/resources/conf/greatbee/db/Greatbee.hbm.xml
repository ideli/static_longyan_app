<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <!--快捷方式-->
    <class name="com.greatbee.bean.EmployeeQuickMenu" table="xiwa_crm_employee_quick_menu" lazy="false">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="name"/>
        <property name="moduleName"/>
        <property name="belongedId"/>
        <property name="createEmployeeId"/>
    </class>

    <!--Portlet-->
    <class name="com.greatbee.bean.Portlet" table="xiwa_crm_portlet" lazy="false">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>

        <property name="name" column="name" type="string"/>
        <property name="description" column="description" type="string"/>
        <property name="alias" column="alias" type="string"/>

        <property name="title" column="title" type="string"/>
        <property name="url" column="url" type="string"/>
        <property name="system" column="system" type="boolean"/>
        <property name="showWeb" column="showWeb" type="boolean"/>

        <property name="employeeId" column="employeeId" type="int"/>
        <property name="portletType" column="portletType" type="string"/>
    </class>

    <!--Employee Porlet-->
    <class name="com.greatbee.bean.EmployeePortlet" table="xiwa_crm_employee_portlet" lazy="false">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="employeeId" column="employeeId" type="int"/>
        <property name="portletId" column="portletId" type="int"/>
    </class>





</hibernate-mapping>